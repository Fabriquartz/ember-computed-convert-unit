{
  "name": "ember-computed-convert-unit",
  "version": "1.0.6",
  "description": "Use convert unit as computed property or decorator",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fabriquartz/ember-computed-convert-unit.git"
  },
  "license": "MIT",
  "author": "Fabriquartz",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "convert-units": "2.3.4",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-htmlbars": "^5.3.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.21.2",
    "ember-cli-cjs-transform": "^2.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.11.0",
    "ember-try": "^1.4.0",
    "ember-welcome-page": "^4.0.0",
    "eslint": "^7.8.0",
    "eslint-plugin-align-assignments": "^1.1.2",
    "eslint-plugin-arca": "^0.9.5",
    "eslint-plugin-ember": "^8.13.0",
    "eslint-plugin-ember-suave": "~1.0.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.4.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": "ember-cli-cjs-transform"
  },
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/Fabriquartz/ember-computed-convert-unit/issues"
  },
  "homepage": "https://github.com/Fabriquartz/ember-computed-convert-unit#readme"
}
